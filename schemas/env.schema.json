{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "cerberus://schemas/env.schema.json",
  "title": "Cerberus Environment",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "CERBERUS_IMPL": {
      "type": "string",
      "enum": [
        "demo",
        "real"
      ]
    },
    "CERBERUS_PROFILE": {
      "type": "string",
      "enum": [
        "baseline",
        "micro_iterations",
        "heavy_verification"
      ]
    },
    "SEARCH_PROVIDER": {
      "type": "string",
      "enum": [
        "dummy",
        "serpapi",
        "bing"
      ],
      "default": "dummy"
    },
    "SERPAPI_KEY": {
      "type": "string"
    },
    "BING_SUBSCRIPTION_KEY": {
      "type": "string"
    },
    "BING_ENDPOINT": {
      "type": "string"
    }
  }
}